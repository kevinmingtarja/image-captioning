{
  "$schema": "https://manifest.hypermode.com/hypermode.json",

  "models": {
    "image-captioning": {
      "sourceModel": "Salesforce/blip-image-captioning-large",
      "provider": "hugging-face",
      "host": "hypermode"
    },
    "embeddings": {
      "sourceModel": "test",
      "host": "openai",
      "path": "v1/embeddings"
